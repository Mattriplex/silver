method Ref__loop_body_54(diz: Ref, current_thread_id: Int, len: Int, b: Seq[Ref], a: Seq[Ref], i: Int, c: Seq[Ref])
  requires diz != null
  requires current_thread_id >= 0
  requires (0 <= i) && (i < len)
  requires |a| == len
  requires acc(a[i].Ref__Integer_value, 1 / 2)
  requires (i == 0) ==> acc(a[i].Ref__Integer_value, 1 / 2)
  requires (i < |a| - 1) ==> acc(a[i + 1].Ref__Integer_value, 1 / 2)
  requires |b| == len
  requires acc(b[i].Ref__Integer_value, 1 / 2)
  requires |c| == len
  requires acc(c[i].Ref__Integer_value, write)
  ensures (0 <= i) && (i < len)
  ensures |a| == len
  ensures acc(a[i].Ref__Integer_value, write)
  ensures |b| == len
  ensures acc(b[i].Ref__Integer_value, 1 / 2)
  ensures |c| == len
  ensures acc(c[i].Ref__Integer_value, write)
{
  var __flatten_4: Ref
  var __flatten_5: Int
  var __flatten_6: Ref
  var __flatten_8: Ref
  var __flatten_9: Int
  var __flatten_10: Ref
  __flatten_4 := a[i]
  __flatten_6 := b[i]
  __flatten_5 := __flatten_6.Ref__Integer_value + 1
    //:: ExpectedOutput(assignment.failed:insufficient.permission)
    // confirmed by Stefan Blom
  __flatten_4.Ref__Integer_value := __flatten_5
  if (i > 0) {
    Ref__recv_body_145(diz, current_thread_id, a, i)
  }
  if (i < len - 1) {
    __flatten_8 := c[i]
    __flatten_10 := a[i + 1]
    __flatten_9 := __flatten_10.Ref__Integer_value + 2
    __flatten_8.Ref__Integer_value := __flatten_9
    Ref__send_body_152(diz, current_thread_id, a, i)
  }
}

