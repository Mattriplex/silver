method Ref__send_body_152(diz: Ref, current_thread_id: Int, a: Seq[Ref], i: Int)
  requires diz != null
  requires current_thread_id >= 0
  requires -1 <= i && i+1 < |a|
  requires acc(a[i + 1].Ref__Integer_value, 1 / 2)
{
  inhale false
