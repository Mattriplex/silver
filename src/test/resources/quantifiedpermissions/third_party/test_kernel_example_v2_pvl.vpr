  ensures acc(diz.Ref__b[tid].Ref__Integer_value, 1 / 4)
  ensures diz.Ref__b[tid].Ref__Integer_value > 0
  ensures acc(diz.Ref__c[tid].Ref__Integer_value, 1 / 4)
  ensures diz.Ref__c[tid].Ref__Integer_value > 0
{
  var __last_barrier: Int
  var __flatten_1: Int
  var __flatten_2: Ref
  var __flatten_3: Int
  var __flatten_4: Ref
  var __flatten_6: Ref
  var __flatten_7: Int
  var __flatten_8: Ref
  var __flatten_9: Ref
  var __flatten_10: Ref
  var __flatten_11: Int
  var __flatten_12: Ref
  __last_barrier := 0
  __flatten_2 := diz.Ref__b[tid]
  __flatten_4 := diz.Ref__c[tid]
  __flatten_3 := __flatten_4.Ref__Integer_value
  __flatten_2.Ref__Integer_value := __flatten_3
  __flatten_1 := Ref__main_barrier(diz, current_thread_id, tcount, gsize, tid, gid, lid, 1, __last_barrier)
  __last_barrier := __flatten_1
  if (tid > 0) {
    __flatten_6 := diz.Ref__a[tid]
    __flatten_8 := diz.Ref__b[tid - 1]
    __flatten_9 := diz.Ref__b[tid]
    __flatten_7 := __flatten_8.Ref__Integer_value + __flatten_9.Ref__Integer_value
    __flatten_6.Ref__Integer_value := __flatten_7
  } else {
    __flatten_10 := diz.Ref__a[tid]
    __flatten_12 := diz.Ref__b[tid]
    __flatten_11 := __flatten_12.Ref__Integer_value
    __flatten_10.Ref__Integer_value := __flatten_11
  }
}

method Ref__main_barrier(diz: Ref, current_thread_id: Int, tcount: Int, gsize: Int, tid: Int, gid: Int, lid: Int, this_barrier: Int, last_barrier: Int) returns (sys__result: Int)
  requires diz != null
