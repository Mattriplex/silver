// chaliceSuite/general-tests/nestedPredicates.chalice
field Nodenext$: Ref

field Nodevalue$: Int

function NodegetNext$(this: Ref): Ref
  requires this != null
  requires acc(Nodevalid$(this), wildcard)
{
  (unfolding acc(Nodevalid$(this), wildcard) in this.Nodenext$)
}

predicate Nodevalid$(this: Ref) {
  acc(this.Nodenext$, wildcard) && acc(this.Nodevalue$, wildcard) && ((this.Nodenext$ != null) ==> acc(Nodevalid$(this.Nodenext$)))
}

predicate Nodep$(this: Ref) {
  acc(this.Nodenext$, wildcard) && acc(this.Nodevalue$, wildcard) && ((this.Nodenext$ != null) ==> acc(Nodeq$(this.Nodenext$)))
}

predicate Nodeq$(this: Ref) {
  acc(this.Nodenext$, wildcard) && acc(this.Nodevalue$, wildcard) && ((this.Nodenext$ != null) ==> acc(Nodep$(this.Nodenext$)))
}

method NodetestNestingUnfold$(this: Ref)
  requires this != null
  requires acc(Nodevalid$(this))
{
  unfold acc(Nodevalid$(this))
  //:: UnexpectedOutput(assert.failed:assertion.false, /Silicon/issue/154/)
  assert this != this.Nodenext$
  if (this.Nodenext$ != null) {
    unfold acc(Nodevalid$(this.Nodenext$))
    assert this.Nodenext$ != this.Nodenext$.Nodenext$
    assert this != this.Nodenext$.Nodenext$
  }
}

method NodetestNestingFold$(this: Ref)
  requires this != null
  requires acc(this.Nodenext$, wildcard) && acc(this.Nodevalue$, wildcard) && acc(this.Nodenext$.Nodenext$, wildcard) && acc(this.Nodenext$.Nodevalue$, wildcard) && (this.Nodenext$ != null) && (this.Nodenext$.Nodenext$ != null) && acc(Nodevalid$(this.Nodenext$.Nodenext$))
{
  fold acc(Nodevalid$(this.Nodenext$))
  //:: UnexpectedOutput(assert.failed:assertion.false, /Silicon/issue/154/)
  assert this.Nodenext$ != this.Nodenext$.Nodenext$
  fold acc(Nodevalid$(this))
  assert this != this.Nodenext$
  assert this != this.Nodenext$.Nodenext$
}

method NodetestNestingUnfolding$(this: Ref)
  requires this != null
  requires acc(Nodevalid$(this))
{
  //:: UnexpectedOutput(assert.failed:assertion.false, /Silicon/issue/154/)
  assert this != (unfolding acc(Nodevalid$(this)) in this.Nodenext$)
  if ((unfolding acc(Nodevalid$(this)) in this.Nodenext$) != null) {
    assert (unfolding acc(Nodevalid$(this)) in this.Nodenext$) != (unfolding acc(Nodevalid$(this)) in (unfolding acc(Nodevalid$(this.Nodenext$)) in this.Nodenext$.Nodenext$))
    assert this != (unfolding acc(Nodevalid$(this)) in (unfolding acc(Nodevalid$(this.Nodenext$)) in this.Nodenext$.Nodenext$))
  }
}

method NodetestNestingUnfoldTwo$(this: Ref)
  requires this != null
  requires acc(Nodep$(this))
{
  unfold acc(Nodep$(this))
  //:: ExpectedOutput(assert.failed:assertion.false)
  assert this != this.Nodenext$
  if (this.Nodenext$ != null) {
    unfold acc(Nodeq$(this.Nodenext$))
    //:: ExpectedOutput(assert.failed:assertion.false)
    //:: MissingOutput(assert.failed:assertion.false, /Silicon/issue/34/)
    assert this.Nodenext$ != this.Nodenext$.Nodenext$
    assert this != this.Nodenext$.Nodenext$
  }
}

